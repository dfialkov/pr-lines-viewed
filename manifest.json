{
  "manifest_version": 3,
  "name": "GitHub Lines Viewed",
  "version": "0.1.0",
  "description": "Shows how many lines you've viewed in a GitHub PR",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
